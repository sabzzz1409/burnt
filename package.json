{
	"name": "burnt",
	"version": "1.0.0",
	"description": "build",
	"main": "src/main.js",
	"scripts": {
		"dev": "cross-env NODE_ENV=dev nodemon myNodeApp.js",
		"try": "cross-env NODE_ENV=prod node myNodeApp.js",
		"app": "cross-env NODE_ENV=prod pm2 start myNodeApp.js",
		"not": "cross-env NODE_ENV=prod pm2 stop myNodeApp.js"
	},
	"imports": {
		"#configs/*": "./src/configs/*",
		"#schemas/*": "./src/repositories/schemas/*",
		"#models/*": "./src/repositories/models/*",
		"#databases/*": "./src/repositories/databases/*",
		"#middlewares/*": "./src/middlewares/*",
		"#scripts/*": "./src/scripts/*",
		"#validators/*": "./src/validators/*"
	},
	"keywords": [],
	"author": "nil",
	"license": "ISC",
	"type": "module",
	"dependencies": {
		"cors": "^2.8.5",
		"cross-env": "^10.0.0",
		"dotenv": "^17.2.2",
		"express": "^5.1.0",
		"express-validator": "^7.2.1",
		"mongoose": "^8.18.1",
		"multer": "^2.0.2",
		"mysql2": "^3.15.0",
		"pm2": "^6.0.11",
		"sql-template-tag": "^5.2.1"
	},
	"devDependencies": {
		"nodemon": "^3.1.10"
	}
}